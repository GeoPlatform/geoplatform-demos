{
  "version": 8,
  "name": "OSM Liberty",
  "metadata": {
    "maputnik:license": "https://github.com/maputnik/osm-liberty/blob/gh-pages/LICENSE.md",
    "maputnik:renderer": "mbgljs",
    "openmaptiles:version": "3.x"
  },
  "sources": {
    "plss": {
      "type": "vector",
      "tiles": [
        "https://sit-tileservice.geoplatform.info/vector/9b59f427_c0ad_5f8b_ac22_2dbdac882dfa/{z}/{x}/{y}.mvt"
      ],
      "minZoom": 0,
      "maxZoom": 14,
      "maxzoom": 22
    }
  },
  "sprite": "https://sit-tileservice.geoplatform.info/assets/sprites/geoplatform",
  "glyphs": "https://sit-tileservice.geoplatform.info/assets/fonts/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "township-fill",
      "type": "fill",
      "source": "plss",
      "source-layer": "PLSSTownship",
      "maxzoom": 24,
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "rgba(121, 159, 112, .5)"}
    },
    {
      "id": "special-survey-fill",
      "type": "fill",
      "source": "plss",
      "source-layer": "PLSSSpecialSurvey",
      "minzoom": 6,
      "maxzoom": 24,
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "rgba(165, 158, 228, 1)",
        "fill-outline-color": "rgba(101, 100, 138, 1)",
        "fill-opacity": 1
      }
    },
    {
      "id": "township",
      "type": "line",
      "source": "plss",
      "source-layer": "PLSSTownship",
      "minzoom": 8,
      "maxzoom": 24,
      "layout": {"visibility": "visible"},
      "paint": {"line-color": "rgba(18, 90, 0, 1)", "line-width": 3}
    },
    {
      "id": "first-division",
      "type": "line",
      "source": "plss",
      "source-layer": "PLSSFirstDivision",
      "minzoom": 9,
      "maxzoom": 24,
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "rgba(129, 68, 42, 1)",
        "line-width": 1,
        "line-opacity": 1
      }
    },
    {
      "id": "second-division",
      "type": "line",
      "source": "plss",
      "source-layer": "PLSSSecondDivision",
      "minzoom": 11,
      "maxzoom": 24,
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "rgba(10, 0, 187, 1)",
        "line-width": 1,
        "line-opacity": 0.5
      }
    },
    {
      "id": "point",
      "type": "circle",
      "source": "plss",
      "source-layer": "PLSSPoint",
      "minzoom": 11,
      "maxzoom": 24,
      "layout": {"visibility": "visible"},
      "paint": {"circle-radius": 3, "circle-color": "rgba(239, 53, 53, 1)"}
    },
    {
      "id": "point-label",
      "type": "symbol",
      "source": "plss",
      "source-layer": "PLSSPoint",
      "minzoom": 12,
      "maxzoom": 24,
      "layout": {
        "text-field": ["get", "POINTLAB"],
        "text-size": 14,
        "visibility": "visible",
        "text-anchor": "bottom-left",
        "text-font": ["Open Sans Regular"],
        "text-offset": [1, -0.5],
        "text-keep-upright": false,
        "text-allow-overlap": false,
        "text-ignore-placement": false,
        "text-padding": 0.5,
        "text-justify": "left"
      },
      "paint": {
        "text-halo-color": "rgba(206, 200, 0, 1)",
        "text-halo-width": 0.75,
        "text-color": "rgba(0, 0, 0, 1)"
      }
    },
    {
      "id": "point-marker",
      "type": "symbol",
      "source": "plss",
      "source-layer": "PLSSPoint",
      "minzoom": 12,
      "layout": {
        "icon-image": "marker",
        "text-offset": [160, 160],
        "icon-offset": [0, -8]
      }
    },
    {
      "id": "township-label",
      "type": "symbol",
      "source": "plss",
      "source-layer": "PLSSTownship",
      "minzoom": 10,
      "layout": {
        "text-font": ["Open Sans Regular"],
        "text-field": ["get", "PLSSID"],
        "symbol-placement": "point",
        "symbol-avoid-edges": true,
        "visibility": "none"
      }
    }
  ],
  "id": "osm-liberty"
}